{
  "expo": {
    "name": "SagaMoent",
    "slug": "sagamoent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/saga-icon.png",
    "userInterfaceStyle": "dark",

    "splash": {
      "image": "./assets/saga-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dk.sagamoent.app"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/saga-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "dk.sagamoent.app"
    },

    "plugins": [
      "expo-build-properties",
      "expo-image-picker",
      "expo-file-system",
      "expo-sqlite",
      "expo-print"
    ],

    "extra": {
      "EXPO_PUBLIC_API_URL": "https://sagacoinbackend-production.up.railway.app",
      "EXPO_PUBLIC_OPENAI_KEY": process.env.EXPO_PUBLIC_OPENAI_KEY
    },

    "runtimeVersion": "1.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}
